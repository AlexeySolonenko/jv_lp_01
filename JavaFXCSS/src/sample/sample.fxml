<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<GridPane fx:id="gridPane"
        fx:controller="sample.Controller"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10"
          stylesheets="@styles.css"
>
    <Button text="Open" onAction="#handleClick" GridPane.rowIndex="0" GridPane.columnIndex="0"
    ></Button>
    <Button text="Button Two" GridPane.rowIndex="0" GridPane.columnIndex="1"></Button>
    <Button id="button_three" text="Button Three" GridPane.rowIndex="0" GridPane.columnIndex="2"

    ></Button>
    <Button fx:id="button4" text="Button Four" GridPane.rowIndex="0" GridPane.columnIndex="3"></Button>
    <Button text="Button Five" GridPane.rowIndex="0" GridPane.columnIndex="4"></Button>
    <RadioButton text="Choice one" GridPane.rowIndex="1" GridPane.columnIndex="0"></RadioButton>
    <Label onMouseEntered="#handleMouseEnter" onMouseExited="#handleMouseExit" fx:id="label"  text="JavaFX Effects" GridPane.rowIndex="1" GridPane.columnIndex="1" rotate="90"></Label>
</GridPane>